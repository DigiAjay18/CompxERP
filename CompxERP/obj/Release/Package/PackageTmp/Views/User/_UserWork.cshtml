@model List<CompxERP.Models.clsUserWork>
<div class="col-sm-12">
    <table id="example1" class="table table-bordered table-striped" style="margin-top: 10px;">
        <thead>
            <tr> 
                <th style="width: 3%;">SNo</th>
                <th style="width: 3%;">Type</th>
                <th style="width: 4%;">Code</th>
                <th style="width: 10%;">VC</th>
                <th style="width: 10%;">Date</th>
                <th style="width: 30%;">Discription</th>
                <th style="width: 20%;">Remark</th>
                <th style="width: 5%;">Mode</th>
                <th style="width: 10%;">Work Date</th>
                <th style="width: 5%;">System</th>
            </tr>
        </thead>
         
    <tbody style="height: 430px;">
        @{
            var iIndex = 0;
            if (Model != null && Model.Count > 0)
            { 
                foreach (var item in Model)
                {
                    iIndex = iIndex + 1; 
            <tr>
              <td style="width: 3%;">@iIndex</td>
                @if(@item.wortype == 0){<td style="width: 3%;">Acct</td>}
                else if(@item.wortype == 174){<td style="width: 3%;">Order</td>}
                else if(@item.wortype == 42){<td style="width: 3%;">SL</td>}
                else if(@item.wortype == 43 ){<td style="width: 3%;">PU</td>}
                else if(@item.wortype == 3){<td style="width: 3%;">RE</td>}
                else if(@item.wortype == 6){<td style="width: 3%;">JV</td>}
                else if(@item.wortype == 5){<td style="width: 3%;">PA</td>}
                else if(@item.wortype == 320){<td style="width: 3%;">OE</td>}
                else if(@item.wortype == 46){<td style="width: 3%;">DN</td>}
                else if(@item.wortype == 81){<td style="width: 3%;">Ret.</td>}
                else if(@item.wortype == 47){<td style="width: 3%;">CN</td>}
                else{ <td style="width: 3%;">@item.wortype</td>}


<td style="width: 4%;">@item.worcode </td>
<td style="width: 10%;">@item.MstChno</td>
<td style="width: 10%;">@item.swordate</td>
<td style="width: 30%;">@item.worrema</td>
<td style="width: 20%;">@item.MstRema</td>
<td style="width: 5%;">@item.sMode</td>
<td style="width: 10%;">@item.sworcudt</td>
<td style="width: 5%;">@item.worsysn</td>
                 
            </tr>
                }
            }
            else
            {
            <tr>
                <td colspan="4">No Records Found</td>
            </tr>            
            }
}
    </tbody>
    </table>
</div>
