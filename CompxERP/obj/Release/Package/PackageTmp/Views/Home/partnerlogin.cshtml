                                    <label class="control-label col-sm-12 padd_l padd_5">User Name</label>
                                    <div class="width_100">
                                      @*  <input type="text" name="uname" class="form-control" />*@
                                        @Html.TextBoxFor(model => model.use_name, new {@class="single-line  form-control"  ,@Value = " " ,@autocomplete=​"off" })

                                    </div>
                                </div>
                                <div>
                                    <label class="control-label col-sm-12 padd_l padd_5">Password</label>

                                    <div class="width_100">
                                        @Html.TextBoxFor(model => model.use_pass, new {@class="single-line2 form-control",   type="password" ,@autocomplete=​"off"})
                                        @*<input type="password" name="pswd" class="form-control" />*@
                                    </div>
                                </div>@*
       
                                <div class="width_100" style="font-size: 12px;"><a href="#" class="fogpass" style="color: #920302; font-weight: bold; text-decoration: none;">Forgot Password?</a></div>
                                <div class="width_100" style="font-size: 12px; color: #666; font-weight: bold;">
                                    <input name="Keep me signed" type="checkbox" value="Keep me signed in" />
                                    &nbsp; &nbsp;Keep me signed in...
                                </div>
         
    *@<br>
                                <div class="width_100">
                                    <br />
                                    <input type="submit" class="btn" value="Sign in" @*data-toggle="modal" id="user_login" title="Sign In"*@ style="width: 100%; color: #fff; box-shadow: none; border-radius: 0; margin-left: 0; background-color: #E31E25;" />
                                    <br>
                                  <br>
                                    <br>
                                   @*   <br>
                                    <span style="float: left; font-size: 14px; margin-top: 10px;">(C) Digiclay</span>
                                    <span style="float: right; font-size: 14px; margin-top: 10px;">(C) Digiclay</span>*@

                                </div>
       
        

                            }
                        </fieldset>
                    </div>
                </div>

            </div>
        </div>

       
    </section>

    <footer id="page-footer" class="text-center">
        <p><span style="margin-top:10px;text-align:center;"><strong>CRM</strong> &copy; copyright 2017</span>
        <img src="~/Img/footer_logoNew.png" class="img-responsive" style="float:right; margin-right:10px;"></p>
    </footer>

    <script>
        function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
        }
    </script>
    <script src='~/js/jquery.min.js'></script>
    <script src="~/js/aos.js"></script>
    <script src="~/js/jquery.scrollto.js"></script>
    <script src="~/js/bootstrap.min.js"></script>

    <script type="text/javascript">
        AOS.init({
            duration: 2000,
        })
    </script>

</body>
</html>