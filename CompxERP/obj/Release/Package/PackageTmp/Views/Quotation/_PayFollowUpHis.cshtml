   
@model List<CompxERP.Models.tblPayFollowUP> 

 <div class="col-sm-12">
     
    <table id="tblPendingOrder" class="table table-bordered table-striped" style="margin-top:10px;margin-bottom:10px;">
    <thead>
      <tr>
            <th style="width:5%;">No</th>  
          <th style="width:16%;">Entry Date</th>
          <th style="width:16%;">Next Date</th> 
          <th style="width:26%;">CommitPay</th> 
          <th style="width:37%;">Remark</th> 
        </tr>
    </thead>
    <tbody style="height:350px;">
          @{
                 var iIndex = 0;   
            if (Model != null && Model.Count > 0)
        { 
            foreach (var item in Model)
            {
                iIndex = iIndex +1;
                  
              <tr>  
                     <td  style="width:5%;">@iIndex </td>    
                  <td  style="width:16%;"> @Convert.ToDateTime(@item.FDate).ToString("dd/MM/yyyy").ToString()  </td>  
                  <td  style="width:16%;"> @Convert.ToDateTime(@item.NextDt).ToString("dd/MM/yyyy") </td>    
                  <td  style="width:26%;text-align:right;">@item.CommitPay </td>  
                     <td  style="width:37%;">@item.Remark  </td>    
                   
                </tr>
           
            }
              
        }
        else
        {
            <tr>
                <td colspan="4">No Records Found</td>
            </tr>            
        }}
    </tbody>
</table>
</div>
 