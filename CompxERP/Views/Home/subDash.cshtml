        function FillMaxNo() {

            $.ajax({
                type: 'POST',
                url: '@Url.Action("FillMaxNoDist", "Home")',
                data: {},
                success: function (data) {

                    if (data.Dis_ID != "") $('#Dis_ID').val(data.Dis_ID); else $('#Dis_ID').val('0');
                    if (data.Deal_ID != "") $('#Deal_ID').val(data.Deal_ID); else $('#Deal_ID').val('0');
                    if (data.Dis_ID != "") $('#lblDistributors').val(data.Dis_ID); else $('#lblDistributors').val('0');
                    if (data.Deal_ID != "") $('#lblDealers').val(data.Deal_ID); else $('#lblDealers').val('0');
                    if (data.TotalSales != "") $('#lblTotalSales').val(data.TotalSales); else $('#lblTotalSales').val('0');
                    if (data.TotalSaleValue != "") $('#lblTotalSaleValue').val(data.TotalSaleValue); else $('#lblTotalSaleValue').val('0');
                    if (data.TotalCollection != "") $('#lblTotalCollection').val(data.TotalCollection); else $('#lblTotalCollection').val('0');
                    if (data.ComplaintSolved != "") $('#lblComplaintSolved').val(data.ComplaintSolved); else $('#lblComplaintSolved').val('0');
                    if (data.PendingComplaint != "") $('#lblPendingComplaint').val(data.PendingComplaint); else $('#lblPendingComplaint').val('0');
                    if (data.RevenFromComplains != "") $('#lblRevenFromComplains').val(parseFloat(data.RevenFromComplains).toFixed(2)); else $('#lblRevenFromComplains').val('0');
                    if (data.Q3Target != "") $('#lblQ3Target').val(data.Q3Target); else $('#lblQ3Target').val('0');
                    if (data.Other != "") $('#lblOther').val(data.Other); else $('#lblOther').val('0');
                    if (data.AvaiStckQty != "") $('#lblAvaiStckQty').val(parseInt(data.AvaiStckQty)); else $('#lblAvaiStckQty').val('0');
                    if (data.ReqStckQty != "") $('#lblReqStckQty').val(parseInt(data.ReqStckQty)); else $('#lblReqStckQty').val('0');
                }
            });
        }
