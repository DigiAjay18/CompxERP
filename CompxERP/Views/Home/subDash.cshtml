    <script type="text/javascript">
        $(document).on('click', function (e) {
            if ($(e.target).closest('.show_hide').length) {
                $(".slidingDiv").slideToggle();
            } else if (!$(e.target).closest('.slidingDiv').length) {
                $('.slidingDiv').hide();
            }
        });

        $(document).ready(function () {
            $(".slidingDiv").delay(200).fadeIn(500);
        });
    </script>
    <style>
        body {
            font-family: Verdana, Geneva, sans-serif;
            font-size: 14px;
        }

        .slidingDiv {
            height: 200px;
            width: 400px;
            background-color: #151515;
            z-index: 999;
            padding: 20px;
            margin-top: 10px;
            border-bottom: 5px solid #3399FF;
            position: fixed;
            bottom: 0;
            right: 0;
            display: none;
            color: #fff;
        }

        .show_hide {
            /*display:none;*/
            float: right;
            position: relative;
            z-index: 99;
            margin-right: 5px;
            border: solid 2px transparent;
            margin-top: 2px;
        }
    </style>
    <!--notification End-->

