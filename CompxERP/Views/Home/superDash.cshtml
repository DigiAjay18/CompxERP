        function FillDegignationList() {

            $.get("FillDegignationList", { iDept: $('#Department_ID').val() }, function (data) {
                if (data != null) {
                    var $table = $('<table class="table table-fixed table-bordered j_table_new table-striped"/>');
                    $table.append('<thead><tr style="ba"><th width="10%">S.No</th><th width="35%">Department</th><th width="35%">Designation</th><th width="10%">&nbsp;</th><th width="10%">&nbsp;</th></tr></thead>');
                    var $tbody = $('<tbody style="height:390px;"/>');


                    $.each(data, function (i, val) {
                        var $row = $('<tr/>');
                        $row.append($('<td width=10%"/>').html(i + 1));
                        $row.append($('<td width="35%"/>').html(val.studadd2));
                        $row.append($('<td width="35%"/>').html(val.StudName));
                        $tbody.append($row);
                    });
                    $table.append($tbody);
                    $('#divDepartmentSetup').html($table);
                }
            });
        }

