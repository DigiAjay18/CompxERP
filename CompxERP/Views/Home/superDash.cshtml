        function getGraphStatisticsData(TypeID) {
            if (TypeID == undefined)
                TypeID = 0;

            $.ajax({
                type: 'POST',
                dataType: 'json',
                contentType: 'application/json',
                url: '@Url.Action("getChartData", "Home")',
                data: "{ 'iType': '" + TypeID + "'}",
                success: function (chartsdata) {
