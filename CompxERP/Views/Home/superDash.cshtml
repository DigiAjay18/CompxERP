        function getGraph(chartsdata) {
            var data = new google.visualization.DataTable();
            var options = {
                'legend': 'left',
                'title': 'My Big Pie Chart',
                'is3D': true,
                'width': 400,
                'height': 300
            };

            data.addColumn('string', 'Remark');
            data.addColumn('number', 'Amt');

            for (var i = 0; i < chartsdata.length; i++) {
                data.addRow([chartsdata[i].Remark, chartsdata[i].Amt]);
                // data.addRow([chartsdata[i].Remark + ' / '+ [chartsdata[i].ItmRemark , chartsdata[i].Amt]);
            }

            var chart;
            chart = new google.visualization.ColumnChart(document.getElementById('chartdiv'));
            chart.draw(data,
              {
                  title: "Order", position: "top", fontsize: "14px", chartArea: { width: '70%' }, 'isStacked': true, 'legend': 'bottom',
                  'vAxis': { 'title': 'O R D E R' },
              });

        }
        function formatNumber(number) {
            if (number != "") {
                number = number.toFixed(2) + '';
                x = number.split('.');
                x1 = x[0];
                x2 = x.length > 1 ? '.' + x[1] : '';
                var rgx = /(\d+)(\d{3})/;
                while (rgx.test(x1)) {
                    x1 = x1.replace(rgx, '$1' + ',' + '$2');
                }

                return x1 + x2;
            }
            return "0";
        }
