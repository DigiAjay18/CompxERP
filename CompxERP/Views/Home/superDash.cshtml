    <style type="text/css">
        header .header_bottom .nav.navbar-nav a {
            padding: 10px 24px !important;
        }

        .right_sidebar .Quick_view.ClientDashboard li .risK_img {
            float: left;
            background: #CC1A57;
            margin-top: 0;
            padding: 4px;
            width: 37%;
            min-height: 76px;
        }

        .right_sidebar .Quick_view.ClientDashboard li .risK_imgTxt {
            min-height: 76px;
        }

        .modal-footer {
            border-top: 1px solid #f2eded;
            text-align: center;
            padding: 10px 0;
        }

        .btn.btn-default {
            font-size: 16px;
        }

        .panel {
            margin-bottom: 10px;
        }

        .right_sidebar .Quick_view > li {
            padding: 5px;
        }

        .dealerList .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
            padding: 3px 8px !important;
        }

        #btnDelete {
            background: #e31e25 !important;
            border: medium none !important;
        }

        body {
            font-family: Verdana, Geneva, sans-serif;
            font-size: 14px;
        }

        .slidingDiv {
            height: 200px;
            width: 400px;
            background-color: #151515;
            z-index: 999;
            padding: 20px;
            margin-top: 10px;
            border-bottom: 5px solid #3399FF;
            position: fixed;
            bottom: 0;
            right: 0;
            display: none;
            color: #fff;
        }

        .show_hide {
            float: right;
            position: relative;
            z-index: 99;
            margin-right: 5px;
            border: solid 2px transparent;
            margin-top: 2px;
        }
    </style>
    <script type="text/javascript">
        google.load("visualization", "1", { packages: ["corechart"] });
        $(document).on('click', function (e) {
            if ($(e.target).closest('.show_hide').length) {
                $(".slidingDiv").slideToggle();
            } else if (!$(e.target).closest('.slidingDiv').length) {
                $('.slidingDiv').hide();
            }
        });
        $(document).ready(function () {
            $(".slidingDiv").delay(200).fadeIn(500);
        });
    </script>
</head>
