@model IEnumerable<CompxERP.Models.clsOrderFollowUp> 
  <link href="~/Content/css/tableexport.min.css" rel="stylesheet" />
<script src="~/Scripts/FileSaver.min.js"></script>
<script src="~/Scripts/tableexport.min.js"></script>

 
 <div class="col-sm-12"> 
    <table id="OrderFollowUpList" class="table table-bordered table-striped" style="margin-top:10px;">
    <thead>
      <tr>
            <th style="width:2%;">No</th> 
            <th style="width:10%;">Entry Date</th>   
          <th style="width:20%;">  Dealer Name</th>
           <th style="width:10%;">  Mobile</th>
          <th style="width:20%;">Last Conversation </th>  
          <th style="width:10%;">Next Date</th>    
        </tr>
    </thead>
    <tbody style="height:430px;">
           @{
                 var iIndex = 0; 
            foreach (var item in Model)
            {
                iIndex = iIndex +1; 
               <tr>  
                     <td  style="width:2%;">@iIndex </td>   
                         <td  style="width:10%;"> @item.FDate</td>  
                    <td style="width:20%;">@item.acctname</td>
                    <td style="width:10%;">@item.acctphon</td> 
                   <td  style="width:20%;"> @item.Remark</td>    
                   <td  style="width:10%;"> @item.NextDt</td>   
                </tr>
            } 
}
    </tbody>
</table>
</div>
      
             
<script>
    $(document).ready(function () {
        $("#OrderFollowUpList").tableExport({
            bootstrap: true, 
        });
         
    }); 
</script>