                debugger;
                var customer = new Array();
                $.ajax({
                    async: false,
                    cache: false,
                    type: "POST",
                    url: "@(Url.Action("GetDealer", "Service"))", 
                    data: { "sName": request.term, IsDealer: $("#hdnIsDealer").val() },
                    success: function (data) {
