 <script >
     $(document).ready(function () {
         try {

             var today = new Date();
             var dd = today.getDate();
             var mm = today.getMonth() + 1; //January is 0!

             var yyyy = today.getFullYear();
             if (dd < 10) {
                 dd = '0' + dd;
             }
             if (mm < 10) {
                 mm = '0' + mm;
             }
             var today = dd + '/' + mm + '/' + yyyy;
             $('#txtC_Date').val(today);

             $(function () {
                 //Datemask dd/mm/yyyy
                 $("#datemask").inputmask("dd/mm/yyyy", { "placeholder": "dd/mm/yyyy" });
                 //Money Euro
                 $("[data-mask]").inputmask();
             });

         } catch (e) { }
     });
    </script>


