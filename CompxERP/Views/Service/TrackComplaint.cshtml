@{
    ViewBag.Title = "TrackComplaint";
}


<div class="modal-dialog modal-md">
    <div class="modal-content">
        <div id="" class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                @* <button type="button" class="close" onclick="ClosePopUpRec()">&times;</button>*@
                <h4 class="modal-title">Track Complaint</h4>
            </div>
            <div class="modal-body input_popup">
                <div class="row">

                    <div class="col-sm-12">
                        <fieldset>
                            <div class="form-group">
                                <label class="control-label col-sm-4 plr5">
                                    Select Complaint No. 
                                </label>
                                <div class="col-sm-8 plr5">
                                   <select>
                                       <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                        <option>4</option>
                                       <option>5</option>
                                   </select>
                                </div>                               
                            </div>
                           <div class="form-group">
                                <label class="control-label col-sm-4 plr5">
                                    Status
                                </label>
                                <div class="col-sm-8 plr5">
                                    <textarea class="form-control"></textarea>
                                </div>                               
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="form-group">
                    <div class="col-sm-12 btn-div-c">
                        <input type="button" value="Save" id="btnComplaint" class="btn btn-default" />
                        <button type="button" onclick="ClearDvData();" class="btn btn-default">Close</button>
                    </div>
                </div>
            </div>
        </div>


    </div>

    </div>


